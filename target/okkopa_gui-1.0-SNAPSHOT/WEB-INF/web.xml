<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="     http://java.sun.com/xml/ns/javaee     http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>ListServlet</servlet-name>
        <servlet-class>fi.helsinki.cs.web.ListServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddReferenceServlet</servlet-name>
        <servlet-class>fi.helsinki.cs.web.AddReferenceServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ListServlet</servlet-name>
        <url-pattern>/List</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddReferenceServlet</servlet-name>
        <url-pattern>/AddReference</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
